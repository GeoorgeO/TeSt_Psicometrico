                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT emp.rh_nombre_candidato, emp.rh_edad, emp.rh_titulo_empleo, zav.t_totl_moral, zav.t_tot_legalidad, zav.t_tot_indiferencia, zav.t_tot_corrupto, zav.t_tot_economico, zav.t_tot_politico, zav.t_tot_social, 
       zav.t_tot_religioso, cle.DM, cle.IM, cle.SM, cle.CM, cle.DL, cle.IL, cle.SL, cle.CL, cle.TOTAL_EMPUJE, cle.TOTAL_INFLUENCIA, cle.TOTAL_CONSTANCIA, cle.TOTAL_CUMPLIMIENTO, mer.Serie1, mer.Serie2, 
       mer.Serie3, mer.Serie4, mer.Serie5, mer.Serie6, mer.Serie7, mer.Serie8, mer.Serie9, mer.Serie10, mer.R1, mer.R2, mer.R3, mer.R4, mer.R5, mer.R6, mer.R7, mer.R8, 
       mer.R9, mer.R10, mer.TOTAL, mer.Ci,mer.RG
FROM   rh_candidatos_empleos AS emp INNER JOIN
       rh_test_zavic AS zav ON emp.id_candidato = zav.id_candidato INNER JOIN
       (SELECT        id_candidato, 
       (CASE tst_empuje_m WHEN 0 THEN 1 WHEN 1 THEN 5 WHEN 2 THEN 10 WHEN 3 THEN 20 WHEN 4 THEN 30 WHEN 5 THEN 40 WHEN 6 THEN 50 WHEN 7 THEN 60 WHEN 8 THEN 65 WHEN 9 THEN
       75 WHEN 10 THEN 84 WHEN 11 THEN 87 WHEN 12 THEN 90 WHEN 13 THEN 93 WHEN 14 THEN 95 WHEN 15 THEN 97 WHEN 16 THEN 97 WHEN 17 THEN 98 WHEN 18 THEN 98 WHEN 19 THEN
       98 WHEN 20 THEN 99 ELSE 0 END) AS DM, 
       (CASE tst_influencia_m WHEN 0 THEN 4 WHEN 1 THEN 10 WHEN 2 THEN 25 WHEN 3 THEN 40 WHEN 4 THEN 55 WHEN 5 THEN 70 WHEN 6 THEN 82 WHEN 7 THEN 90 WHEN 8 THEN 95 WHEN 9
       THEN 96 WHEN 10 THEN 97 WHEN 11 THEN 97 WHEN 12 THEN 97 WHEN 13 THEN 97 WHEN 14 THEN 97 WHEN 15 THEN 97 WHEN 16 THEN 97 WHEN 17 THEN 99 ELSE 0 END) AS IM, 
       (CASE tst_constancia_m WHEN 0 THEN 5 WHEN 1 THEN 10 WHEN 2 THEN 16 WHEN 3 THEN 30 WHEN 4 THEN 40 WHEN 5 THEN 55 WHEN 6 THEN 63 WHEN 7 THEN 75 WHEN 8 THEN 84 WHEN
       9 THEN 90 WHEN 10 THEN 95 WHEN 11 THEN 96 WHEN 12 THEN 97 WHEN 13 THEN 97 WHEN 14 THEN 97 WHEN 15 THEN 97 WHEN 16 THEN 98 WHEN 17 THEN 98 WHEN 18 THEN 98 WHEN 19
       THEN 99 ELSE 0 END) AS SM, 
       (CASE tst_cumplimiento_m WHEN 0 THEN 1 WHEN 1 THEN 5 WHEN 2 THEN 16 WHEN 3 THEN 30 WHEN 4 THEN 55 WHEN 5 THEN 70 WHEN 6 THEN 84 WHEN 7 THEN 93 WHEN 8 THEN 95 WHEN
       9 THEN 97 WHEN 10 THEN 97 WHEN 11 THEN 97 WHEN 12 THEN 98 WHEN 13 THEN 98 WHEN 14 THEN 98 WHEN 15 THEN 99 ELSE 0 END) AS CM, 
       (CASE tst_empuje_l WHEN 0 THEN 99 WHEN 1 THEN 95 WHEN 2 THEN 87 WHEN 3 THEN 80 WHEN 4 THEN 65 WHEN 5 THEN 55 WHEN 6 THEN 50 WHEN 7 THEN 35 WHEN 8 THEN 30 WHEN 9 THEN
       20 WHEN 10 THEN 18 WHEN 11 THEN 15 WHEN 12 THEN 10 WHEN 13 THEN 6 WHEN 14 THEN 5 WHEN 15 THEN 4 WHEN 16 THEN 3 WHEN 17 THEN 2 WHEN 18 THEN 2 WHEN 19 THEN 2 WHEN
       20 THEN 2 WHEN 21 THEN 1 ELSE 0 END) AS DL, 
       (CASE tst_influencia_l WHEN 0 THEN 99 WHEN 1 THEN 95 WHEN 2 THEN 87 WHEN 3 THEN 75 WHEN 4 THEN 55 WHEN 5 THEN 40 WHEN 6 THEN 25 WHEN 7 THEN 16 WHEN 8 THEN 10 WHEN 9
       THEN 5 WHEN 10 THEN 4 WHEN 11 THEN 4 WHEN 12 THEN 3 WHEN 13 THEN 3 WHEN 14 THEN 3 WHEN 15 THEN 2 WHEN 16 THEN 2 WHEN 17 THEN 2 WHEN 18 THEN 2 WHEN 19 THEN 1 ELSE
       0 END) AS IL, 
       (CASE tst_constancia_l WHEN 0 THEN 99 WHEN 1 THEN 97 WHEN 2 THEN 95 WHEN 3 THEN 87 WHEN 4 THEN 80 WHEN 5 THEN 65 WHEN 6 THEN 55 WHEN 7 THEN 35 WHEN 8 THEN 28 WHEN
       9 THEN 18 WHEN 10 THEN 10 WHEN 11 THEN 5 WHEN 12 THEN 4 WHEN 13 THEN 3 WHEN 14 THEN 3 WHEN 15 THEN 3 WHEN 16 THEN 2 WHEN 17 THEN 2 WHEN 18 THEN 2 WHEN 19 THEN 1
       ELSE 0 END) AS SL, 
       (CASE tst_cumplimiento_l WHEN 0 THEN 99 WHEN 1 THEN 97 WHEN 2 THEN 95 WHEN 3 THEN 90 WHEN 4 THEN 84 WHEN 5 THEN 70 WHEN 6 THEN 55 WHEN 7 THEN 40 WHEN 8 THEN 38 WHEN
       9 THEN 23 WHEN 10 THEN 10 WHEN 11 THEN 5 WHEN 12 THEN 4 WHEN 13 THEN 3 WHEN 14 THEN 2 WHEN 15 THEN 2 WHEN 16 THEN 1 ELSE 0 END) AS CL, 
       (CASE (tst_empuje_m - tst_empuje_l) 
       WHEN - 21 THEN 1 WHEN - 20 THEN 2 WHEN - 19 THEN 2 WHEN - 18 THEN 2 WHEN - 17 THEN 2 WHEN - 16 THEN 2 WHEN - 15 THEN 2 WHEN - 14 THEN 2 WHEN - 13 THEN 4 WHEN - 12 THEN 5
       WHEN - 11 THEN 5 WHEN - 10 THEN 9 WHEN - 9 THEN 13 WHEN - 8 THEN 15 WHEN - 7 THEN 16 WHEN - 6 THEN 20 WHEN - 5 THEN 25 WHEN - 4 THEN 29 WHEN - 3 THEN 35 WHEN - 2 THEN 40
       WHEN - 1 THEN 45 WHEN 0 THEN 50 WHEN 1 THEN 55 WHEN 2 THEN 60 WHEN 3 THEN 65 WHEN 4 THEN 67 WHEN 5 THEN 70 WHEN 6 THEN 75 WHEN 7 THEN 80 WHEN 8 THEN 84 WHEN 9 THEN
       85 WHEN 10 THEN 90 WHEN 11 THEN 91 WHEN 12 THEN 94 WHEN 13 THEN 95 WHEN 14 THEN 96 WHEN 15 THEN 97 WHEN 16 THEN 97 WHEN 17 THEN 98 WHEN 18 THEN 98 WHEN 19 THEN
       98 WHEN 20 THEN 99 ELSE 0 END) AS TOTAL_EMPUJE, (CASE (tst_influencia_m - tst_influencia_l) 
       WHEN - 19 THEN 1 WHEN - 18 THEN 2 WHEN - 17 THEN 2 WHEN - 16 THEN 2 WHEN - 15 THEN 2 WHEN - 14 THEN 2 WHEN - 13 THEN 2 WHEN - 12 THEN 2 WHEN - 11 THEN 2 WHEN - 10 THEN 3
       WHEN - 9 THEN 4 WHEN - 8 THEN 5 WHEN - 7 THEN 6 WHEN - 6 THEN 10 WHEN - 5 THEN 16 WHEN - 4 THEN 20 WHEN - 3 THEN 29 WHEN - 2 THEN 35 WHEN - 1 THEN 45 WHEN 0 THEN 55 WHEN
       1 THEN 60 WHEN 2 THEN 70 WHEN 3 THEN 75 WHEN 4 THEN 85 WHEN 5 THEN 90 WHEN 6 THEN 95 WHEN 7 THEN 96 WHEN 8 THEN 97 WHEN 9 THEN 97 WHEN 10 THEN 98 WHEN 11 THEN 98
       WHEN 12 THEN 98 WHEN 13 THEN 98 WHEN 14 THEN 98 WHEN 15 THEN 98 WHEN 16 THEN 98 WHEN 17 THEN 99 ELSE 0 END) AS TOTAL_INFLUENCIA, 
       (CASE (tst_constancia_m - tst_constancia_l) 
       WHEN - 19 THEN 1 WHEN - 18 THEN 2 WHEN - 17 THEN 2 WHEN - 16 THEN 2 WHEN - 15 THEN 2 WHEN - 14 THEN 2 WHEN - 13 THEN 2 WHEN - 12 THEN 3 WHEN - 11 THEN 4 WHEN - 10 THEN 5
       WHEN - 9 THEN 8 WHEN - 8 THEN 10 WHEN - 7 THEN 15 WHEN - 6 THEN 20 WHEN - 5 THEN 25 WHEN - 4 THEN 30 WHEN - 3 THEN 35 WHEN - 2 THEN 40 WHEN - 1 THEN 50 WHEN 0 THEN 57 WHEN
       1 THEN 60 WHEN 2 THEN 70 WHEN 3 THEN 75 WHEN 4 THEN 80 WHEN 5 THEN 84 WHEN 6 THEN 87 WHEN 7 THEN 91 WHEN 8 THEN 94 WHEN 9 THEN 96 WHEN 10 THEN 97 WHEN 11 THEN 97
       WHEN 12 THEN 98 WHEN 13 THEN 98 WHEN 14 THEN 98 WHEN 15 THEN 98 WHEN 16 THEN 98 WHEN 17 THEN 98 WHEN 18 THEN 98 WHEN 19 THEN 99 ELSE 0 END) AS TOTAL_CONSTANCIA,
       (CASE (tst_cumplimiento_m - tst_cumplimiento_l) 
       WHEN - 16 THEN 1 WHEN - 15 THEN 2 WHEN - 14 THEN 2 WHEN - 13 THEN 2 WHEN - 12 THEN 2 WHEN - 11 THEN 3 WHEN - 10 THEN 4 WHEN - 9 THEN 6 WHEN - 8 THEN 9 WHEN - 7 THEN 13 WHEN
       - 6 THEN 20 WHEN - 5 THEN 25 WHEN - 4 THEN 35 WHEN - 3 THEN 40 WHEN - 2 THEN 55 WHEN - 1 THEN 60 WHEN 0 THEN 70 WHEN 1 THEN 75 WHEN 2 THEN 84 WHEN 3 THEN 90 WHEN 4 THEN
       95 WHEN 5 THEN 96 WHEN 6 THEN 97 WHEN 7 THEN 97 WHEN 8 THEN 98 WHEN 9 THEN 98 WHEN 10 THEN 98 WHEN 11 THEN 98 WHEN 12 THEN 98 WHEN 13 THEN 98 WHEN 14 THEN 98 WHEN
       15 THEN 99 ELSE 0 END) AS TOTAL_CUMPLIMIENTO
FROM   rh_test_cleaver) AS cle ON zav.id_candidato = cle.id_candidato INNER JOIN
 (SELECT id_candidato, CANDIDATO, FECHA, EDAD, PSOLICITADO, Serie1, Serie2, Serie3, Serie4, Serie5, Serie6, Serie7, Serie8, Serie9, Serie10,R1,R2,R3,R4,R5,R6,R7,R8,R9,R10,TOTAL,Ci,RG
